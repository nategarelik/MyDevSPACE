# Database
DATABASE_URL="postgresql://user:password@localhost:5432/ultimate_ai_ide"
SUPABASE_URL="your-supabase-project-url"
SUPABASE_ANON_KEY="your-supabase-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-supabase-service-role-key"

# AI APIs
CLAUDE_API_KEY="your-claude-api-key"
OPENAI_API_KEY="your-openai-api-key"
ANTHROPIC_API_KEY="your-anthropic-api-key"

# Redis
REDIS_URL="redis://localhost:6379"

# NextAuth.js
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret"

# GitHub (for repository integration)
GITHUB_TOKEN="your-github-personal-access-token"

# Notion (for workspace integration)
NOTION_TOKEN="your-notion-integration-token"

# Slack (for communication integration)
SLACK_BOT_TOKEN="your-slack-bot-token"
SLACK_SIGNING_SECRET="your-slack-signing-secret"

# Development
NODE_ENV="development"
LOG_LEVEL="debug"

# MCP Servers Configuration
MCP_SERVER_TIMEOUT=30000
MCP_SERVER_MAX_RETRIES=3
MCP_SERVER_RETRY_DELAY=1000

# Application Settings
APP_NAME="Ultimate AI IDE"
APP_VERSION="0.1.0"
APP_DESCRIPTION="The Ultimate AI-Powered Development Environment"
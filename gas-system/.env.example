# GAS (Garelik Agentic System) Environment Configuration
# Copy this file to .env and add your actual API keys

# AI Model API Keys
CLAUDE_API_KEY=your_claude_api_key_here
OPENAI_API_KEY=your_openai_api_key_here  
GOOGLE_AI_KEY=your_gemini_api_key_here

# Budget Limits (USD)
GAS_BUDGET_DAILY=10
GAS_BUDGET_MONTHLY=200

# System Configuration
GAS_LOG_LEVEL=info
GAS_VERBOSE=false
GAS_DEBUG=false

# Dashboard Configuration
DASHBOARD_PORT=3000
DASHBOARD_HOST=localhost

# Memory Configuration  
MEMORY_RETENTION=permanent
MEMORY_ENCRYPTION=true

# Security Configuration
SECURITY_LEVEL=high
AUDIT_LOGGING=true
CREDENTIAL_STORE=local

# Performance Configuration
MAX_CONCURRENT_REQUESTS=4
REQUEST_TIMEOUT=30000
CACHE_ENABLED=true

# Personal Configuration (for Nathaniel Garelik)
OWNER_NAME="Nathaniel Garelik"
OWNER_TIMEZONE="America/New_York"
WORKING_HOURS="9:00-17:00"
PREFERRED_LANGUAGE="English"